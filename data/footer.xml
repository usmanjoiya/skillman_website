<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <template id="dash_footer" inherit_id="website.footer_custom" name="Footer">
        <xpath expr="//div[@id='footer']" position="replace">

            <style>
                footer{
                background:#ACADAF;
                }
                .footer_imgs{
                display:flex;
                width:100%;
                justify-content:space-between;
                padding: 50px 50px 20px 50px;
                }
                .footer_para{
                display:flex;
                width:100%;
                justify-content:space-between;
                padding: 0px 50px 50px 50px;
                }
                .footer_para_left{
                font-size:13px;
                }

                @media (max-width:576px) {
                .footer_imgs{
                flex-direction:column;
                width:100%;
                }
                .footer_para{
                flex-direction:column;
                width:100%;
                }
                .footer_right_img{
                margin-top:25px;
                }

                }


            </style>
            <footer>
                <div class="footer_imgs">
                    <div class="footer_left_img">
                        <img src="/skillman_website/static/src/images/cofundedeu (1).png" alt="" height="70px"
                             style=""/>
                    </div>
                    <div class="footer_right_img">

                        <img src="/skillman_website/static/src/images/skillmansecretariat-e1642498801517 (1).png" alt=""
                             height="70px" style=""/>
                    </div>

                </div>

                <div class="footer_para">
                    <div class="footer_para_left">
                        <p>
                            The European Commission's support for the production of this publication does not constitute
                            an <br/> endorsement of the contents, which reflect the views only of the authors, and the
                            Commission cannot be <br/> held responsible for any use which may be made of the information
                            contained therein.
                        </p>

                    </div>
                    <div class="footer_para_right">
                        <p>
                            Legal notice on GDPR privacy compliance
                        </p>

                    </div>

                </div>
            </footer>

        </xpath>
    </template>
</odoo>


